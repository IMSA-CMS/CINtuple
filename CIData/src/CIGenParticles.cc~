//Include for the header file 
#include "ZprimeDiLeptons/CIData/interface/CIGenParticles.h"

//Includes for special types 
#include "DataFormats/PatCandidates/interface/PackedGenParticle.h"

CIGenParticles::CIGenParticles(int NbGenMuons, pat::PackedGenParticle p):
  iGen(NbGenMuons),
  idGen(p.pdgId()),
  statusGen(p.status()),
  ptGen(p.pt()),
  etaGen(p.eta()),
  phiGen(p.phi()),
  chargeGen(p.charge()),
  energyGen(p.energy()),
  pxGen(p.px()),				
  pyGen(p.py()),
  pzGen(p.pz())
{}
